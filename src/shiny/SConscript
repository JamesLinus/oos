# vim: syntax=python

Import('env', 'arch', 'buildtype', 'distreq')

sources = []

shiny = env.Clone()

Export('sources', 'shiny')

SConscript('boot/%s/SConscript' % arch)

sources += ['init/init.c']

#distreq += ['#src/shiny/shiny.exe']

shinyexe = env.Program('shiny', sources)

distreq += [shinyexe]
